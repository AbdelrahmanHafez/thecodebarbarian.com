    div.container-fluid
      div.col-lg-3.col-lg-offset-9.right-bar.hidden-md.hidden-sm.hidden-xs
        .right-bar-content-slider.pull-right
          .row.search-container
              .col-lg-12
                .form-group
                label Search
                form(onsubmit="window.location.href = 'http://blekko.com/ws/?q=' + encodeURIComponent(document.getElementById('searchQ').value) +  ' /vkarpov15/thecodebarbarian'; return false;",)
                  .input-group
                    input.form-control#searchQ(type="text")
                    .input-group-addon.search-button
                      i.fa.fa-search
          .row.recent-posts.right-bar-group
              .col-lg-12
                p.right-bar-label Recent Posts
                ul.list-unstyled
                  li.right-bar-li Introduction to the Mean Stack
                  li.right-bar-li How to Validate Any Form Ever Using MongooseJS
                  li.right-bar-li Some Paleo Nonsense
          .row.recent-posts.right-bar-group
              .col-lg-12
                p.right-bar-label Popular Posts
                ul.list-unstyled
                  li.right-bar-li Introduction to the Mean Stack
                  li.right-bar-li How to Validate Any Form Ever Using MongooseJS
                  li.right-bar-li Some Paleo Nonsense
    div.container-fluid.hidden-sm.hidden-md.hidden-lg#mobile-sharing-options
      .row
          .col-lg-12
            .post-sharing-options
              .row
                .col-xs-3.twitter-share.sharing-option
                  i.fa.fa-twitter
                .col-xs-3.facebook-share.sharing-option
                  i.fa.fa-facebook
                .col-xs-3.google-share.sharing-option
                  i.fa.fa-google-plus
                .col-xs-3.comment.sharing-option
                  i.fa.fa-comment

